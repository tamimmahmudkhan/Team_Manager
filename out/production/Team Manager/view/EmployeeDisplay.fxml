<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="employeeDisplay" alignment="CENTER" prefWidth="320.0" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.EmployeeDisplayController">
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="75.0" prefWidth="320.0" text="Employees">
         <font>
            <Font name="Times New Roman Bold" size="23.0" />
         </font>
      </Label>
      <Label fx:id="nameLabel" prefHeight="17.0" prefWidth="331.0" text="Name:" />
      <Label fx:id="dobLabel" layoutX="20.0" layoutY="142.0" prefHeight="17.0" prefWidth="331.0" text="Date of Birth:" />
      <Label fx:id="positionLabel" layoutX="20.0" layoutY="169.0" prefHeight="17.0" prefWidth="331.0" text="Position:" />
      <Label fx:id="workDayLabel" layoutX="20.0" layoutY="196.0" prefHeight="17.0" prefWidth="331.0" text="Working Days:" />
      <Label fx:id="absentDayLabel" layoutX="20.0" layoutY="223.0" prefHeight="17.0" prefWidth="331.0" text="Absent Days:" />
      <Label fx:id="projectLabel" layoutX="20.0" layoutY="223.0" prefHeight="17.0" prefWidth="331.0" text="Project:" />
      <Label fx:id="teamLabel" layoutX="20.0" layoutY="286.0" prefHeight="17.0" prefWidth="331.0" text="Team: " />
      <Button fx:id="editButton" mnemonicParsing="false" onAction="#editMenu" prefHeight="25.0" prefWidth="302.0" text="Edit" />
   </children>
   <padding>
      <Insets bottom="20.0" left="10.0" right="10.0" top="20.0" />
   </padding>
</VBox>
